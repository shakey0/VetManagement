<h1>All Staff</h1>

<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Position</th>
            <th>Admin</th>
        </tr>
    </thead>

    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.first_name %></td>
            <td><%= user.last_name %></td>
            <td><%= user.email %></td>
            <td><%= user.phone %></td>
            <td><%= user.position %></td>
            <td><%= user.is_admin ? 'Yes' : 'No' %></td>
            <td>
            </td>
        </tr>
        <% end %>
    </tbody>
</table>
